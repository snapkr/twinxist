#!/webapps/hello_django/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'Scrapy==1.0.0','console_scripts','scrapy'
__requires__ = 'Scrapy==1.0.0'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('Scrapy==1.0.0', 'console_scripts', 'scrapy')()
    )
